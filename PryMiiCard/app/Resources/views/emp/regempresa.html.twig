{% extends "::empresas.html.twig"%}
{% block title %}Registro Empresas{% endblock %}
{% block titulo %} Registro de Empresas{%  endblock %}
{% block cuerpo %}

    <div class ="row">
        <!-- Nav tabs -->
        <ul class="nav nav-tabs" role="tablist">
            <li role="presentation" class="active"><a href="#ingreso" aria-controls="ingreso" role="tab" data-toggle="tab">Registro</a></li>
        </ul>

        <style>
            #div1 {background-color: #f4f4f4; margin: 2px; width: 1000px; height: 40px;}
        </style>

        <!-- Tab panes -->
        <div class="tab-content">
            <div role="tabpanel" class="tab-pane active" id="ingreso">
                {{ form_start(form) }}
                {% form_theme form 'bootstrap_3_horizontal_layout.html.twig' %}
                <form>
                    <div class="form-group" id="div1">
                        {{ form_row(form.nomEmp) }}
                    </div>
                    <div class="form-group" id="div1">
                        {{ form_row(form.usuEmp) }}
                    </div>
                    <div class="form-group" id="div1">
                        {{ form_row(form.pasEmp) }}
                    </div>
                    <div class="form-group" id="div1">
                        {{ form_row(form.rucEmp) }}
                    </div>
                    <div class="form-group" id="div1">
                        {{ form_row(form.tlfEmp) }}
                    </div>
                    <div class="form-group" id="div1">
                        {{ form_row(form.webEmp) }}
                    </div>
                    <div class="form-group" id="div1">
                        {{ form_row(form.logEmp) }}
                    </div>
                    <div class="form-group" id="div1">
                        {{ form_row(form.slgEmp) }}
                    </div>
                </form>
                {{ form_end(form) }}
            </div>
            <div role="tabpanel" class="tab-pane" id="modificacion">...</div>
        </div>
    </div>

{% endblock %}